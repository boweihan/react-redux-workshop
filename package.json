{
  "name": "react-minesweeper",
  "homepage": "https://davidbanksnz.github.io/react-redux-minesweeper",
  "version": "0.1.0",
  "dependencies": {
    "material-design-iconic-font": "^2.2.0",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.0.0",
    "node-sass-chokidar": "0.0.3",
    "react-scripts": "1.0.11",
    "redux-devtools-extension": "^2.13.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run build-css",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "node ./deployment/deploy.js",
    "build-css": "node-sass-chokidar src/assets/styles/styles.scss -o src/assets/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles/styles.scss -o src/assets/styles/ --watch --recursive"
  }
}
